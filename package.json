{
    "name": "baklavajs",
    "version": "0.6.0",
    "description": "Node/Graph editor for the browser using VueJS",
    "keywords": [
        "node",
        "graph",
        "editor",
        "blender"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/newcat/baklavajs"
    },
    "files": [
        "dist/**/!(report.html)"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build-lib": "webpack",
        "gen-doc": "typedoc",
